        <div class="header_top">
            <div class="container">
                <div class="contact_nav">
                    <a href="">
                        <i class="fa fa-phone" aria-hidden="true"></i>
                        <span>
                            Call : +84 139617683
                        </span>
                    </a>
                    <a href="">
                        <i class="fa fa-envelope" aria-hidden="true"></i>
                        <span>
                            Email : nghiadatduyson@gmail.com
                        </span>
                    </a>
                    <a href="">
                        <i class="fa fa-map-marker" aria-hidden="true"></i>
                        <span>
                            280 An Duong Vuong, District 5, Ho Chi Minh City
                        </span>
                    </a>
                </div>
            </div>
        </div>
        <div class="header_bottom">
            <div class="container-fluid">
                <nav class="navbar navbar-expand-lg custom_nav-container ">
                    <a class="navbar-brand" asp-action="Index">
                        <img src="/images/logo.png" alt="">
                    </a>


                    @*  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="">123123123123321312312312</span>
                    </button> *@

                    <div class="collapse navbar-collapse" id="navbarSupportedContent">
                        <div class="d-flex mr-auto flex-column flex-lg-row align-items-center">
                            <ul class="navbar-nav  ">
                                <li class="nav-item active">
                                    <a class="nav-link" asp-action="Index">Home <span class="sr-only">(current)</span></a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" asp-action="About"> About</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" asp-action="Treatment">Treatment</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" asp-action="Doctor">Doctors</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" asp-action="Testimonial">Testimonial</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" asp-page="/BAppointment">Booked Appointment</a>
                                </li>
                            </ul>
                        </div>
                        <div class="quote_btn-container">
                            <a href="">
                                <i class="fa fa-user" aria-hidden="true"></i>
                                <span>
                                    Login for ADMIN
                                </span>
                            </a>
                        </div>
                    </div>
                </nav>
            </div>
        </div>
    </header>

    <div>
        @RenderBody()
    </div>

    <!-- info section -->
    <section class="info_section mt-4">
        <div class="container">
            <div class="info_top">
                <div class="info_logo">
                    <a asp-action="Index">
                        <img src="/images/logo.png" alt="">
                    </a>
                </div>
            </div>
            <div class="info_bottom layout_padding2 ">
                <div class="row justify-content-center info_main_row">
                    <div class="col-md-6 col-lg-3">
                        <h5>
                            Address
